{
  "name": "minipac",
  "version": "2.2.4",
  "type": "module",
  "description": "Minimalistic web application bundler",
  "repository": "git@github.com:TheLudd/minipac.git",
  "author": "Ludwig Magnusson <ludwig.magnusson@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/minipac.cjs",
  "exports": {
    ".": "./dist/minipac.cjs",
    "./server": "./dist/minipac-server.cjs"
  },
  "devDependencies": {
    "@yafu/eslint-config-yafu": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.41.0",
    "mocha": "^6.2.2",
    "@esbuild-kit/cjs-loader": "^2.4.2"
  },
  "scripts": {
    "transpile": "esbuild index.js --bundle --platform=node --packages=external --outfile=dist/minipac.cjs && esbuild server.js --bundle --platform=node --packages=external --outfile=dist/minipac-server.cjs",
    "prepublish": "yarn lint",
    "lint": "eslint lib server.js",
    "unit": "mocha",
    "test": "yarn unit"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@fal-works/esbuild-plugin-global-externals": "^2.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babelify": "^10.0.0",
    "bl": "^3.0.0",
    "browserify": "^16.5.2",
    "connect-livereload": "^0.6.1",
    "cookie-parser": "^1.4.6",
    "envify": "^4.1.0",
    "esbuild": "^0.17.19",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "exorcist": "^1.0.1",
    "express": "^4.18.2",
    "fs-extra": "^8.1.0",
    "less": "^2.7.3",
    "less-plugin-npm-import": "^2.1.0",
    "livereload": "^0.8.2",
    "minify-stream": "^2.1.0",
    "multipipe": "^4.0.0",
    "pug": "^2.0.4",
    "ramda": "^0.26.1",
    "string-hash": "^1.1.3",
    "watchify": "^3.11.1",
    "yafu": "^1.1.0"
  }
